{
    "name": "cowlby/cloud-identity-bindings",
    "description": "PHP bindings for the Rackspace Cloud Identity api.",
    "type": "library",
    "keywords": ["rackspace cloud", "cloud identity", "api bindings"],
    "license": "MIT",

    "authors": [
        {
            "name": "Jose Prado",
            "email": "git@cowlby.com"
        }
    ],

    "minimum-stability": "dev",

    "require": {
        "php": ">=5.3.2",
        "cowlby/cloud-bindings-common": "dev-master",
        "guzzle/guzzle": "*",
        "pimple/pimple": "dev-master",
        "symfony/serializer": "2.1.*"
    },

    "require-dev": {
        "monolog/monolog": "1.*"
    },

    "autoload": {
        "psr-0": {
            "Cowlby\\Rackspace\\Cloud\\Identity": "src/",
            "Cowlby\\Tests\\Rackspace\\Cloud\\Identity": "tests/"
        }
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cowlby/cloud-bindings-common"
        }
    ]
}
